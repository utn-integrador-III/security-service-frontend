# CONFIGURACIÓN NPM PARA VERSIONES FIJAS
# No actualizar automáticamente packages
save-exact=true
package-lock=true
shrinkwrap=false

# Evitar actualizaciones de Tailwind CSS
# tailwindcss=3.4.16
# postcss=8.5.6  
# autoprefixer=10.4.21

# Configuración de instalación
prefer-offline=true
cache-max=86400000
